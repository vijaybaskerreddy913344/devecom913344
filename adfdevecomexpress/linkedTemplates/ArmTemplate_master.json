					"LS_SQL_PARQUET_properties_typeProperties_url": {
						"value": "[parameters('LS_SQL_PARQUET_properties_typeProperties_url')]"
					},
					"LS_SI_PAYMENTS_properties_typeProperties_url": {
						"value": "[parameters('LS_SI_PAYMENTS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfdevecomexpress_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
